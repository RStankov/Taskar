<ul>
  <% @project.events.each do |event| %>
    <li>
      <%= event.action %>
      <%= event.subject_type %>
      <%= link_to event.info, :controller => event.subject_type.downcase.pluralize, :id => event.subject_id, :action => :show %>
      <%= event.user.full_name %>
    </li>
  <% end %>
</ul>

<ul id="events">
  <% for i in 1..10 %>
  <li<%= i % 4 == 0 ? ' class="personal"' : '' %>>
    <a href="#"><%= image_tag "temp/ra4oil.png" %></a>
    <p><a href="#">Radoslav Stankov</a> closed <a href="#">Taskar layout</a>, opened <a href="#">Dashboard design</a></p>
    <span>about 12 hours ago</span>
  </li>
  <% end %>
</ul>
<h1><%=t :'users.title' %></h1>
<%= link_to t(:'layouts.projects.new_user'), new_user_path %>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= image_tag user.avatar.url %>
      <strong><%= link_to h(user.full_name), user %></strong><br />
      <small><%= user.email %></small>
    </li>
  <% end %>
</ul>

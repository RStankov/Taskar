<header>
	<h1><%=t '.title' %></h1>
</header>
<div class="wrap">
	<% if @projects[0] %>
		<h2><%=t '.active' %></h2>
		<ul>
			<%= render @projects %>
		</ul>
	<% end %>
	<hr />
	<% if @completed[0] %>
		<h2><%=t '.completed' %></h2>
		<ul>
			<%= render @completed %>
		</ul>
		<hr />
	<% end %>
</div>
<header>
	<h1>Projects</h1>
</header>
<div class="wrap">
	<h2>Active Projects</h2>
	<ul>
		<% @projects.each do |project|%>
			<li><%= link_to project.name, project %></li>
		<% end %>
	</ul>
	<h2>Completed Projects</h2>
	<ul>
		<% @completed.each do |project|%>
			<li><%= link_to project.name, project %></li>
		<% end %>
	</ul>
</div>